{
  "name": "termaite",
  "version": "0.2.0",
  "description": "Multi-agent AI CLI wrapper with intelligent rotation, fallback, and conversation management",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "commonjs",
  "bin": {
    "termaite": "src/index.cjs"
  },
  "files": [
    "src/",
    "README.md",
    "LICENSE",
    "termaite.jpg"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/index.cjs",
    "publish:patch": "node scripts/publish.js patch",
    "publish:minor": "node scripts/publish.js minor",
    "publish:major": "node scripts/publish.js major",
    "publish:patch:dry": "node scripts/publish.js patch --dry-run",
    "publish:minor:dry": "node scripts/publish.js minor --dry-run",
    "publish:major:dry": "node scripts/publish.js major --dry-run",
    "changelog": "node scripts/publish.js patch --dry-run",
    "prepare": "git config core.hooksPath .githooks"
  },
  "keywords": [
    "ai",
    "cli",
    "agent",
    "wrapper",
    "claude",
    "gemini",
    "qwen",
    "cursor",
    "llm",
    "multi-agent",
    "rotation",
    "failover"
  ],
  "author": "Patrick",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daxiongmao87/termaite.git"
  },
  "homepage": "https://github.com/Daxiongmao87/termaite#readme",
  "dependencies": {
    "blessed": "^0.1.81",
    "yargs": "^18.0.0"
  }
}
